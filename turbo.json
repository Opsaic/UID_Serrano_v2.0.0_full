{
  "$schema": "https://turborepo.com/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_KEY",
        "OPENAI_API_KEY"
      ],
      "outputs": [
        "apps/web/.next/**",
        "apps/web/.next/cache/**"
      ]
    }
  },
  "globalEnv": [
    "GITHUB_TOKEN"
  ],
  "globalDependencies": [
    "turbo.json",
    "package-lock.json"
  ]
}
